<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="weprosever.model.Video" table="video" schema="car">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="videoId">
            <column name="video_id" sql-type="varchar(100)" length="100"/>
        </property>
        <property name="title">
            <column name="title" sql-type="varchar(120)" length="120"/>
        </property>
        <property name="url">
            <column name="url" sql-type="varchar(100)" length="100"/>
        </property>
        <property name="userId">
            <column name="user_id" sql-type="varchar(100)" length="100"/>
        </property>
        <property name="imgUrl">
            <column name="img_url" sql-type="varchar(100)" length="100"/>
        </property>
    </class>
</hibernate-mapping>